    "istanbul": "~0.3.17",
    "jshint": "*",
    "mocha": "3",
    "should": "7"
  },
  "homepage": "https://github.com/node-modules/urlencode",
  "repository": {
    "type": "git",
    "url": "git://github.com/node-modules/urlencode.git"
  },
  "keywords": [
    "urlencode",
    "urldecode",
